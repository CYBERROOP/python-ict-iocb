   "source": [
    "def f(x):\n",
    "    return x**3 - x - 2\n",
    "\n",
    "def bisection(a, b):\n",
    "    while abs(b-a)>0.0001:\n",
    "        c = (a + b) / 2\n",
    "        if f(c) == 0:\n",
    "            break\n",
    "        elif f(a) * f(c) < 0:\n",
    "            b = c\n",
    "        else:\n",
    "            a = c\n",
    "    print(\"Root =\", c)\n",
    "\n",
    "bisection(1, 2)"
   ]
